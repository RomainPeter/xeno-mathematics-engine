      "id": "lemma1",
      "kind": "lemma",
      "label": "Yoneda Lemma",
      "data": {
        "statement": "For any functor F: C^op → Set, there is a natural isomorphism Hom(Hom(-, c), F) ≅ F(c)"
      }
    },
    {
      "id": "proof1",
      "kind": "proof",
      "label": "Natural transformation construction",
      "data": {
        "method": "natural_transformation",
        "steps": ["define_phi", "check_naturality", "verify_bijection"]
      }
    },
    {
      "id": "corollary1",
      "kind": "corollary",
      "label": "Representable functors",
      "data": {
        "consequence": "F is representable iff F ≅ Hom(-, c) for some c"
      }
    }
