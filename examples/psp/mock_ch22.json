{
  "blocks": [
    {
      "id": "def1",
      "kind": "definition",
      "label": "Homotopy equivalence",
      "data": {
        "definition": "Two spaces X and Y are homotopy equivalent if there exist continuous maps f: X → Y and g: Y → X such that g∘f ≃ id_X and f∘g ≃ id_Y"
      }
    },
    {
      "id": "lemma1",
      "kind": "lemma",
      "label": "Contractible spaces",
      "data": {
        "statement": "A space is contractible if and only if it is homotopy equivalent to a point"
      }
    },
    {
      "id": "proof1",
      "kind": "proof",
      "label": "Contractible implies homotopy equivalent to point",
      "data": {
        "method": "direct_construction",
        "key_idea": "Use the constant map and the homotopy to construct the equivalence"
      }
    },
    {
      "id": "theorem1",
      "kind": "theorem",
      "label": "Fundamental group of contractible space",
      "data": {
        "statement": "If X is contractible, then π₁(X, x₀) is trivial for any basepoint x₀"
      }
    },
    {
      "id": "proof2",
      "kind": "proof",
      "label": "Trivial fundamental group",
      "data": {
        "method": "homotopy_invariance",
        "steps": ["contract_to_point", "apply_homotopy_invariance", "conclude_trivial"]
      }
    }
  ],
